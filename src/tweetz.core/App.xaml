<Application
  x:Class="tweetz.core.App"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  Startup="ApplicationStartup">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Resources/Dark.xaml" />
        <ResourceDictionary Source="Resources/Strings.xaml" />
        <ResourceDictionary Source="Resources/Symbols.xaml" />
        <ResourceDictionary Source="Resources/ToolTip.xaml" />
        <ResourceDictionary Source="Resources/Button.xaml" />
        <ResourceDictionary Source="Resources/TextBlock.xaml" />
        <ResourceDictionary Source="Resources/Hyperlink.xaml" />
        <ResourceDictionary Source="Resources/ScrollBar.xaml" />
        <ResourceDictionary Source="Resources/Twitter.xaml" />
        <ResourceDictionary Source="Resources/Converters.xaml" />
        <ResourceDictionary Source="Resources/TweetBlockStyles.xaml" />
        <ResourceDictionary Source="Resources/UserProfileBlockStyles.xaml" />
        <ResourceDictionary Source="Resources/TweetImageControlStyles.xaml" />
        <ResourceDictionary Source="Resources/TabControlStyles.xaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>