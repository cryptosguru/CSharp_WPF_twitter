<UserControl
  x:Class="tweetz.core.Controls.ComposeBlock.ComposeBlockImageListControl"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:local="clr-namespace:tweetz.core.Controls.ComposeBlock"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  d:DesignHeight="100"
  d:DesignWidth="300"
  mc:Ignorable="d">

  <ItemsControl ItemsSource="{Binding Mode=OneWay}">
    <ItemsControl.ItemTemplate>
      <DataTemplate>
        <local:ComposeBlockImageControl DataContext="{Binding Mode=OneWay}" />
      </DataTemplate>
    </ItemsControl.ItemTemplate>
  </ItemsControl>
</UserControl>